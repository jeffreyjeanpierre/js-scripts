set = set.split('\n')

d = new Map()
set.forEach((arr)=> {
  item = arr.split(':')
  d.set(item[0], false)
})

obj = JSON.parse(JSON.stringify([...d]))

o = {}; obj.forEach((v)=>{o[v[0]] = v[1] })